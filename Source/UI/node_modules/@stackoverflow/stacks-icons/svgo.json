{
    "multipass": true,
    "plugins": [
        {
            "convertPathData": {
                "floatPrecision": 2,
                "transformPrecision": 4
            }
        },
        {
            "cleanupNumericValues": {
                "floatPrecision": 2
            }
        },
        {
            "collapseGroups": true
        },
        {
            "removeTitle": true
        },
        {
            "removeViewBox": false
        },
        {
            "removeUselessStrokeAndFill": true
        },
        {
            "removeAttrs": {
                "attrs": [
                    "xmlns",
                    "fill-rule",
                    "clip-rule"
                ]
            }
        },
        {
            "mergePaths": {
                "force": true
            }
        }
    ]
}