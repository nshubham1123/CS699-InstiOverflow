{
    "plugins": ["jsdoc-route-plugin","better-docs/component"],
    
    "source": {
        "include":["UI"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    

    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "recurse":true,
        "destination":"./docs_UI/"
    }
}